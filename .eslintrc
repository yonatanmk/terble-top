{
  "extends": [
    "airbnb"
  ],
  "root": true,
  "rules": {
    "import/prefer-default-export": "off",
    "arrow-parens": ["error", "as-needed"],
    "no-tabs": "off",
    "indent": "off",
    "function-paren-newline": "off",
    "max-len": "off",
    "no-console": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "consistent-return": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "global-require": "off"
  }
}
